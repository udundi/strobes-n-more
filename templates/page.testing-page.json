n-items: center;\n    gap: 4px;\n    transition: color 0.2s ease;\n    font-family: 'Nunito Sans', sans-serif;\n  }\n\n  .popup-link:hover {\n    color: #333;\n    text-decoration: underline;\n  }\n\n  .popup-link::after {\n    content: '>';\n    font-weight: bold;\n  }\n\n  /* Position hotspots - you can adjust these coordinates */\n  .hotspot-1 {\n    top: 26%;\n    left: 53%;\n  }\n\n  .hotspot-2 {\n    top: 45%;\n    right: 12%;\n  }\n\n  .hotspot-3 {\n    top: 65%;\n    left: 21%;\n  }\n\n  .hotspot-4 {\n    top: 55%;\n    right: 71%;\n  }\n\n  .hotspot-5 {\n    top: 43%;\n    left: 54%;\n  }\n\n  /* Dynamic popup positioning - will be controlled by JavaScript */\n  .hotspot-popup.position-above {\n    bottom: 100%;\n    margin-bottom: 10px;\n    transform: translateY(-10px);\n  }\n\n  .hotspot-popup.position-below {\n    top: 100%;\n    margin-top: 10px;\n    transform: translateY(10px);\n  }\n\n  .hotspot-popup.position-left {\n    left: 50%;\n    transform: translateX(-50%) translateY(-10px);\n  }\n\n  .hotspot-popup.position-right {\n    right: 0;\n    transform: translateY(-10px);\n  }\n\n  .hotspot:hover .hotspot-popup.position-above.position-left {\n    transform: translateX(-50%) translateY(0);\n  }\n\n  .hotspot:hover .hotspot-popup.position-above.position-right {\n    transform: translateY(0);\n  }\n\n  .hotspot:hover .hotspot-popup.position-below.position-left {\n    transform: translateX(-50%) translateY(0);\n  }\n\n  .hotspot:hover .hotspot-popup.position-below.position-right {\n    transform: translateY(0);\n  }\n\n  /* Responsive adjustments */\n  @media (max-width: 768px) {\n    .vehicle-hotspots-section {\n      height: 70vh;\n      min-height: 400px;\n    }\n    \n    .hotspot {\n      width: 16px;\n      height: 16px;\n    }\n    \n    .hotspot-popup {\n      min-width: 150px;\n      padding: 10px 12px;\n    }\n    \n    .popup-heading {\n      font-size: 13px;\n    }\n  }\n</style>\n\n<section class=\"vehicle-hotspots-section\">\n  <!-- Hots